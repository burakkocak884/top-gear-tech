<h1><%= @customer.first_name %>'s vehicles:</h1>
<ul>
<% @vehicles.each  do |v| %>
<li><%= link_to "#{v.year} #{v.make} #{v.model} ", vehicle_path(v)%> </li>
<% end %>
<li><%= link_to "Add a Vehicle", new_vehicle_path(@customer.id) %></li>

<li><%= link_to 'Back to Garages', garages_path(@user) %></li>
</ul>



