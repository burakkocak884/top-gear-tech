

<%= form_for(@service) do |f| %>


<%= f.label :name %>
<%= f.text_field :name %></br>

 

<%= f.label :description %>
<%= f.text_field :description %></br>

<%= f.label :total_cost %>
<%= f.number_field :total_cost %></br>


<%= f.label :vehicle_id %>
<%= f.collection_select :vehicle_id,  Vehicle.all, :id,  :license_plate %></br>


<%= f.submit  %>
<%end%>